{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "projectType": "application",
      "schematics": {
        "@ionic/angular-toolkit:page": {
          "styleext": "scss",
          "standalone": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "www",
              "browser": ""
            },
            "index": "src/index.html",
            "polyfills": ["src/polyfills.ts"],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "assets"
              },
              {
                "glob": "**/*.svg",
                "input": "node_modules/ionicons/dist/ionicons/svg",
                "output": "./svg"
              },
              {
                "glob": "manifest.webmanifest",
                "input": "src",
                "output": "/"
              },
              {
                "glob": "firebase-messaging-sw.js",
                "input": "src",
                "output": "/"
              }
            ],
            "styles": ["src/global.scss", "src/theme/variables.scss"],
            "scripts": [],
            "browser": "src/main.ts",
            "serviceWorker": "ngsw-config.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true,
              "budgets": [
                { "type": "initial", "maximumWarning": "2000kb", "maximumError": "2500kb" },
                { "type": "anyComponentStyle", "maximumWarning": "20kb", "maximumError": "40kb" }
              ]
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "abdc3c01-d7c2-46f7-acd3-628ab1428f46"
  }
}
